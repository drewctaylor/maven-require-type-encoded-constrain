    <parent>
        <groupId>io.github.drewctaylor</groupId>
        <artifactId>maven</artifactId>
        <version>0.0.1-SNAPSHOT</version>
    </parent>

    <artifactId>function-javapoet</artifactId>
    <version>0.0.1-SNAPSHOT</version>
